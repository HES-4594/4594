  labs(title="HLA DPB (YRI)", caption = "(Based on data from Hudjashov et al., 2017)")+
  coord_polar("y")+
  guides(fill = guide_legend(title = "HLA DPB genes"))+
  scale_fill_manual(values=group.colors.HLADPB, limits=force)+
  theme_void()
ggsave("HLA-DPB YRI.png")
ggplot(HLA_DQA_YRI, aes(x="", y=MAF, fill=reorder(SNP, -MAF))) +
  geom_bar(stat="identity", color="black", width=1) +
  labs(title="HLA DQA (YRI)", caption = "(Based on data from Hudjashov et al., 2017)")+
  coord_polar("y")+
  guides(fill = guide_legend(title = "HLA DQA genes"))+
  scale_fill_manual(values=group.colors.HLADQA, limits=force)+
  theme_void()
ggsave("HLA-DQA YRI.png")
ggplot(HLA_DQB_YRI, aes(x="", y=MAF, fill=reorder(SNP, -MAF))) +
  geom_bar(stat="identity", color="black", width=1) +
  labs(title="HLA DQB (YRI)", caption = "(Based on data from Hudjashov et al., 2017)")+
  coord_polar("y")+
  guides(fill = guide_legend(title = "HLA DQB genes"))+
  scale_fill_manual(values=group.colors.HLADQB, limits=force)+
  theme_void()
ggsave("HLA-DQB YRI.png")
ggplot(HLA_DR_YRI, aes(x="", y=MAF, fill=reorder(SNP, -MAF))) +
  geom_bar(stat="identity", color="black", width=1) +
  labs(title="HLA DR (YRI)", caption = "(Based on data from Hudjashov et al., 2017)")+
  coord_polar("y")+
  guides(fill = guide_legend(title = "HLA DR genes"))+
  scale_fill_manual(values=group.colors.HLADR, limits=force)+
  theme_void()
ggsave("HLA-DR YRI.png")

#Create stand-alone legends
#HLA-A
LegendHLAA <- ggplot(HLA_A_AlorAF, aes(x="", y=MAF, fill=reorder(SNP, -MAF)))+
  geom_bar(stat="identity", color="black", width=1)+
  labs(title="HLA A (AlorAF)")+
  coord_polar("y")+
  guides(fill = guide_legend(title = NULL))+
  scale_fill_manual(values=group.colors.HLAA)+
  theme_void()

legendHLAA <- cowplot::get_legend(LegendHLAA)

grid.newpage()
grid.draw(legendHLAA)
ggsave("HLA_A legend.png", bg="transparent")


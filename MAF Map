  mutate(rank = rank(-MAF), 
         SNP = ifelse(rank <= 5, SNP, 'Other'))
HLA_DPA_TimULaAF <- HLA_DPA_TimULaAF %>% 
  mutate(rank = rank(-MAF), 
         SNP = ifelse(rank <= 5, SNP, 'Other'))
HLA_DPB_TimULaAF <- HLA_DPB_TimULaAF %>% 
  mutate(rank = rank(-MAF), 
         SNP = ifelse(rank <= 5, SNP, 'Other'))
HLA_DR_TimULaAF <- HLA_DR_TimULaAF %>% 
  mutate(rank = rank(-MAF), 
         SNP = ifelse(rank <= 5, SNP, 'Other'))
HLA_DQA_TimULaAF <- HLA_DQA_TimULaAF %>% 
  mutate(rank = rank(-MAF), 
         SNP = ifelse(rank <= 5, SNP, 'Other'))
HLA_DQB_TimULaAF <- HLA_DQB_TimULaAF %>% 
  mutate(rank = rank(-MAF), 
         SNP = ifelse(rank <= 5, SNP, 'Other'))


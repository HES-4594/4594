#Create pie charts

#AlorAF
ggplot(HLA_A_AlorAF, aes(x="", y=MAF, fill=reorder(SNP, -MAF)))+
  geom_bar(stat="identity", color="black", width=1)+
  labs(title="HLA A (AlorAF)")+
  coord_polar("y")+
  guides(fill = guide_legend(title = NULL))+
  scale_fill_manual(values=group.colors.HLAA, limits=force)+
  theme_void()
ggsave("HLA-A AlorAF.png", bg="transparent")
ggplot(HLA_B_AlorAF, aes(x="", y=MAF, fill=reorder(SNP, -MAF)))+

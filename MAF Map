##MAF pie charts
setwd()
#Read data

HLA_A <- read.table("HLA-ASnpFrequencies.frq.strat", header=T)
HLA_B <- read.table("HLA-BSnpFrequencies.frq.strat", header=T)
HLA_C <- read.table("HLA-CSnpFrequencies.frq.strat", header=T)
HLA_DR <-read.table("HLA-DRSnpFrequencies.frq.strat", header=T)
HLA_DPA <-read.table("HLA-DPASnpFrequencies.frq.strat", header=T)
HLA_DPB <-read.table("HLA-DPBSnpFrequencies.frq.strat", header=T)
HLA_DQA <-read.table("HLA-DQASnpFrequencies.frq.strat", header=T)
HLA_DQB <-read.table("HLA-DQBSnpFrequencies.frq.strat", header=T)

#Remove specific HLA proteins
HLA_A <- HLA_A[!grepl(":", HLA_A$SNP),]
HLA_B <- HLA_B[!grepl(":", HLA_B$SNP),]
HLA_C <- HLA_C[!grepl(":", HLA_C$SNP),]
HLA_DR <- HLA_DR[!grepl(":", HLA_DR$SNP),]
HLA_DPA <- HLA_DPA[!grepl(":", HLA_DPA$SNP),]
HLA_DPB <- HLA_DPB[!grepl(":", HLA_DPB$SNP),]
HLA_DQA <- HLA_DQA[!grepl(":", HLA_DQA$SNP),]
HLA_DQB <- HLA_DQB[!grepl(":", HLA_DQB$SNP),]

#Filter by specific family ID

HLA_A_AlorAF <- filter(HLA_A, CLST=="AlorAF")
HLA_B_AlorAF <- filter(HLA_B, CLST=="AlorAF")
HLA_C_AlorAF <- filter(HLA_C, CLST=="AlorAF")
HLA_DR_AlorAF <- filter(HLA_DR, CLST=="AlorAF")
HLA_DPA_AlorAF <- filter(HLA_DPA, CLST=="AlorAF")

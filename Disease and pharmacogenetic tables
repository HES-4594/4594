HBVvfreqTimULa <- setDT(HLAfreqTimULa)[SNP %chin% HBV_vac$Gene] 
write.table(HBVvfreqTimULa, file="HBVvTimULa.txt", quote=FALSE, row.names = FALSE) 
MalariafreqTimULa <-setDT(HLAfreqTimULa)[SNP %chin% Malaria$HLA.gene]
write.table(MalariafreqTimULa, file="MalariaTimULa.txt", quote=FALSE, row.names = FALSE)
MalariaVfreqTimULa <-MalariaVfreqTimULa <- setDT(HLAfreqTimULa)[SNP %chin% Malaria_vac$Gene]
write.table(MalariaVfreqTimULa, file="MalariaVTimULa.txt", quote=FALSE, row.names = FALSE)
TBfreqTimULa <- setDT(HLAfreqTimULa)[SNP %chin% TB$HLA.gene]
write.table(TBfreqTimULa, file="TBTimULa.txt", quote=FALSE, row.names = FALSE)
TBmfreqTimULa <-setDT(HLAfreqTimULa)[SNP %chin% TB_med$Gene]
write.table(TBmfreqTimULa, file="TBmTimULa.txt", quote=FALSE, row.names = FALSE)

#Vietnam 
HBVfreqVietnam <- setDT(HLAfreqVietnam)[SNP %chin% HBV$HLA.gene]
write.table(HBVfreqVietnam, file="HBVVietnam.txt", quote=FALSE, row.names = FALSE)
HBVmfreqVietnam <- setDT(HLAfreqVietnam)[SNP %chin% HBV_med$Gene]
write.table(HBVmfreqVietnam, file="HBVmVietnam.txt", quote=FALSE, row.names = FALSE)
HBVvfreqVietnam <- setDT(HLAfreqVietnam)[SNP %chin% HBV_vac$Gene] 
write.table(HBVvfreqVietnam, file="HBVvVietnam.txt", quote=FALSE, row.names = FALSE) 
MalariafreqVietnam <-setDT(HLAfreqVietnam)[SNP %chin% Malaria$HLA.gene]
write.table(MalariafreqVietnam, file="MalariaVietnam.txt", quote=FALSE, row.names = FALSE)
MalariaVfreqVietnam <-MalariaVfreqVietnam <- setDT(HLAfreqVietnam)[SNP %chin% Malaria_vac$Gene]
write.table(MalariaVfreqVietnam, file="MalariaVVietnam.txt", quote=FALSE, row.names = FALSE)
TBfreqVietnam <- setDT(HLAfreqVietnam)[SNP %chin% TB$HLA.gene]
write.table(TBfreqVietnam, file="TBVietnam.txt", quote=FALSE, row.names = FALSE)
TBmfreqVietnam <-setDT(HLAfreqVietnam)[SNP %chin% TB_med$Gene]
write.table(TBmfreqVietnam, file="TBmVietnam.txt", quote=FALSE, row.names = FALSE)

#YRI 

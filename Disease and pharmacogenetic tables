    "Sumba (Wun)" = Sumba..Wun.,
    "Timor (Kam)" = Timor..Kam.,
    "Timor (ULa)" = Timor..ULa.,
    "Yuruba (Nigeria)" = Yuruba..Nigeria.
  )

#Remove non-Indonesian populations
TBM <- TBM[, c("SNP","Alor", "Bali", "Borneo", "Flores Bama", "Flores Bena", "Flores Rampasasa", "Java", "Lembata", "Malay", "Mentawai", "Nias", "Pantar", "Papua New Guinea (Baining)", "Papua New Guinea (Coastal)", "Papua New Guinea (Highland)", "Papua New Guinea (Nasioi)", "Sulawesi", "Sumatra", "Sumba (Ana)", "Sumba (Rin)", "Sumba (Wun)", "Timor (Kam)", "Timor (ULa)")]

#Renaming column headers 
TBM <- data.frame(t(TBM))
TBM <- tibble::rownames_to_column(TBM,"Population")
TBM <- janitor::row_to_names(TBM, row_number = 1)
TBM <- TBM %>%
  rename(
    "Population"=SNP,
  )

#Making data numeric
TBM <- TBM %>%
  mutate_at(c(2:8), as.numeric) 

#Manually removing alleles below exclusion threshold
TBM$`HLA_B*14`=NULL

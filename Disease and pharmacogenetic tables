##Descriptive tables##
#Set working directory
setwd()
#Read data with HLA associations
TB <- read.csv("TB.csv", header=TRUE)
TB_med <- read.csv("TB_med.csv", header=TRUE)
Malaria <- read.csv("Malaria.csv", header=TRUE)
Malaria_vac <- read.csv("Malaria_vac.csv", header=TRUE)
HBV <- read.csv("HBV.csv", header=TRUE)
HBV_med <- read.csv("HBV_med.csv", header=TRUE)
HBV_vac <- read.csv("HBV_vac.csv", header=TRUE)

#Read HLA frequency files
HLAfreq <- read.table("LOCATION/HLASnpFrequencies.frq.strat", header=TRUE)
HLAfreqAlor <- read.table("LOCATION/HLAAlorSnpFrequencies.frq.strat", header=TRUE)
HLAfreqBali <- read.table("LOCATION/HLABaliSnpFrequencies.frq.strat", header=TRUE)
HLAfreqBEB <- read.table("LOCATION/HLABEBSnpFrequencies.frq.strat", header=TRUE)
HLAfreqBorneo <- read.table("LOCATION/HLABorneoSnpFrequencies.frq.strat", header=TRUE)
HLAfreqCDX <- read.table("LOCATION/HLACDXSnpFrequencies.frq.strat", header=TRUE)
HLAfreqCEU <- read.table("LOCATION/HLACEUSnpFrequencies.frq.strat", header=TRUE)
HLAfreqCHB <- read.table("LOCATION/HLACHBSnpFrequencies.frq.strat", header=TRUE)
HLAfreqCHS <- read.table("LOCATION/HLACHSSnpFrequencies.frq.strat", header=TRUE)
HLAfreqFloBamAF <- read.table("LOCATION/HLAFloBamAFSnpFrequencies.frq.strat", header=TRUE)
HLAfreqFloBenAF <- read.table("LOCATION/HLAFloBenAFSnpFrequencies.frq.strat", header=TRUE)
HLAfreqFloRamAF <- read.table("LOCATION/HLAFloRamAFSnpFrequencies.frq.strat", header=TRUE)
HLAfreqGIH <- read.table("LOCATION/HLAGIHSnpFrequencies.frq.strat", header=TRUE)
HLAfreqITU <- read.table("LOCATION/HLAITUSnpFrequencies.frq.strat", header=TRUE)
HLAfreqJavaAF <- read.table("LOCATION/HLAJavaAFSnpFrequencies.frq.strat", header=TRUE)
HLAfreqJPT <- read.table("LOCATION/HLAJPTSnpFrequencies.frq.strat", header=TRUE)
HLAfreqKHV <- read.table("LOCATION/HLAKHVSnpFrequencies.frq.strat", header=TRUE)
HLAfreqLembat <- read.table("LOCATION/HLALembatSnpFrequencies.frq.strat", header=TRUE)
HLAfreqMalay <- read.table("LOCATION/HLAMalaySnpFrequencies.frq.strat", header=TRUE)
HLAfreqMentaw <- read.table("LOCATION/HLAMentawSnpFrequencies.frq.strat", header=TRUE)
HLAfreqNias <- read.table("LOCATION/HLANiasSnpFrequencies.frq.strat", header=TRUE)
HLAfreqPantar <- read.table("LOCATION/HLAPantarSnpFrequencies.frq.strat", header=TRUE)
HLAfreqPHNegr <- read.table("LOCATION/HLAPHNegrSnpFrequencies.frq.strat", header=TRUE)
HLAfreqPHIgorTR <- read.table("LOCATION/HLAPHIgorTRSnpFrequencies.frq.strat", header=TRUE)
HLAfreqPHOther <- read.table("LOCATION/HLAPHOtherSnpFrequencies.frq.strat", header=TRUE)
HLAfreqPJL <- read.table("LOCATION/HLAPJLSnpFrequencies.frq.strat", header=TRUE)
HLAfreqPNGBai <- read.table("LOCATION/HLAPNGBaiSnpFrequencies.frq.strat", header=TRUE)
HLAfreqPNGCAF <- read.table("LOCATION/HLAPNGCAFSnpFrequencies.frq.strat", header=TRUE)
HLAfreqPNGHAF <- read.table("LOCATION/HLAPNGHAFSnpFrequencies.frq.strat", header=TRUE)
HLAfreqPNGNas <- read.table("LOCATION/HLAPNGNasSnpFrequencies.frq.strat", header=TRUE)
HLAfreqSTU <- read.table("LOCATION/HLASTUSnpFrequencies.frq.strat", header=TRUE)
HLAfreqSulawesi <- read.table("LOCATION/HLASulawesiSnpFrequencies.frq.strat", header=TRUE)
HLAfreqSumAna <- read.table("LOCATION/HLASumAnaSnpFrequencies.frq.strat", header=TRUE)
HLAfreqSumatra <- read.table("LOCATION/HLASumatraSnpFrequencies.frq.strat", header=TRUE)
HLAfreqSumRin <- read.table("LOCATION/HLASumRinSnpFrequencies.frq.strat", header=TRUE)
HLAfreqSumWun <- read.table("LOCATION/HLASumWunSnpFrequencies.frq.strat", header=TRUE)
HLAfreqTaiwan <- read.table("LOCATION/HLATaiwanSnpFrequencies.frq.strat", header=TRUE)
HLAfreqTimKam <- read.table("LOCATION/HLATimKamSnpFrequencies.frq.strat", header=TRUE)
HLAfreqTimULa <- read.table("LOCATION/HLATimULaSnpFrequencies.frq.strat", header=TRUE)
HLAfreqVietnam <- read.table("LOCATION/HLAVietnamSnpFrequencies.frq.strat", header=TRUE)
HLAfreqYRI <- read.table("LOCATION/HLAYRISnpFrequencies.frq.strat", header=TRUE)

##Identify HLA frequencies relevant to each disease/pharmacogenetic implications by populations
#HBV
HBVfreq <- setDT(HLAfreq)[SNP %chin% HBV$HLA.gene]

#Alor 

HBVfreqAlor <- setDT(HLAfreqAlor)[SNP %chin% HBV$HLA.gene]
write.table(HBVfreqAlor, file="HBVAlor.txt", quote=FALSE, row.names = FALSE)
HBVmfreqAlor <- setDT(HLAfreqAlor)[SNP %chin% HBV_med$Gene]
write.table(HBVmfreqAlor, file="HBVmAlor.txt", quote=FALSE, row.names = FALSE)
HBVvfreqAlor <- setDT(HLAfreqAlor)[SNP %chin% HBV_vac$Gene] 
write.table(HBVvfreqAlor, file="HBVvAlor.txt", quote=FALSE, row.names = FALSE) 
MalariafreqAlor <-setDT(HLAfreqAlor)[SNP %chin% Malaria$HLA.gene]
write.table(MalariafreqAlor, file="MalariaAlor.txt", quote=FALSE, row.names = FALSE)
MalariaVfreqAlor <-MalariaVfreqAlor <- setDT(HLAfreqAlor)[SNP %chin% Malaria_vac$Gene]
write.table(MalariaVfreqAlor, file="MalariaVAlor.txt", quote=FALSE, row.names = FALSE)
TBfreqAlor <- setDT(HLAfreqAlor)[SNP %chin% TB$HLA.gene]
write.table(TBfreqAlor, file="TBAlor.txt", quote=FALSE, row.names = FALSE)
TBmfreqAlor <-setDT(HLAfreqAlor)[SNP %chin% TB_med$Gene]
write.table(TBmfreqAlor, file="TBmAlor.txt", quote=FALSE, row.names = FALSE)  

#Bali 
HBVfreqBali <- setDT(HLAfreqBali)[SNP %chin% HBV$HLA.gene]
write.table(HBVfreqBali, file="HBVBali.txt", quote=FALSE, row.names = FALSE)
HBVmfreqBali <- setDT(HLAfreqBali)[SNP %chin% HBV_med$Gene]
write.table(HBVmfreqBali, file="HBVmBali.txt", quote=FALSE, row.names = FALSE)

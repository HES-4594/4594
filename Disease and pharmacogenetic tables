TBmfreqJPT <- TBmfreqJPT %>% 
  rename("Japanese"="MAF")
TBmfreqKHV <- TBmfreqKHV[, c("MAF")]
TBmfreqKHV <- TBmfreqKHV %>% 
  rename("KHV (Vietnam)"="MAF")
TBmfreqLembat <- TBmfreqLembat[, c("MAF")]
TBmfreqLembat <- TBmfreqLembat %>% 
  rename("Lembata"="MAF")
TBmfreqMalay <- TBmfreqMalay[, c("MAF")]
TBmfreqMalay <- TBmfreqMalay %>% 
  rename("Malay"="MAF")
TBmfreqMentaw <- TBmfreqMentaw[, c("MAF")]
TBmfreqMentaw <- TBmfreqMentaw %>% 
  rename("Mentawai"="MAF")
TBmfreqNias <- TBmfreqNias[, c("MAF")]
TBmfreqNias <- TBmfreqNias %>% 
  rename("Nias"="MAF")
TBmfreqPantar <- TBmfreqPantar[, c("MAF")]
TBmfreqPantar <- TBmfreqPantar %>% 
  rename("Pantar"="MAF")
TBmfreqPHNegr <- TBmfreqPHNegr[, c("MAF")]
TBmfreqPHNegr <- TBmfreqPHNegr %>% 
  rename("Negritos (Philippines)"="MAF")
TBmfreqPHOther <- TBmfreqPHOther[, c("MAF")]
TBmfreqPHOther <- TBmfreqPHOther %>% 
  rename("Non-Kankanaey (Philippines)"="MAF")
TBmfreqPHIgorTR <- TBmfreqPHIgorTR[, c("MAF")]
TBmfreqPHIgorTR <- TBmfreqPHIgorTR %>% 
  rename("Kankanaey (Philippines"="MAF")
TBmfreqPJL <- TBmfreqPJL[, c("MAF")]
TBmfreqPJL <- TBmfreqPJL %>% 
  rename("Punjabi (Pakistan)"="MAF")
TBmfreqPNGBai <- TBmfreqPNGBai[, c("MAF")]
TBmfreqPNGBai <- TBmfreqPNGBai %>% 
  rename("PNG Baining"="MAF")
TBmfreqPNGCAF <- TBmfreqPNGCAF[, c("MAF")]
TBmfreqPNGCAF <- TBmfreqPNGCAF %>% 
  rename("PNG Coastal"="MAF")
TBmfreqPNGHAF <- TBmfreqPNGHAF[, c("MAF")]
TBmfreqPNGHAF <- TBmfreqPNGHAF %>% 
  rename("PNG Highlands"="MAF")
TBmfreqPNGNas <- TBmfreqPNGNas[, c("MAF")]
TBmfreqPNGNas <- TBmfreqPNGNas %>% 
  rename("PNG Nasioi"="MAF")
TBmfreqSTU <- TBmfreqSTU[, c("MAF")]
TBmfreqSTU <- TBmfreqSTU %>% 
  rename("Tamil (Sri Lanka)"="MAF")
TBmfreqSulawesi <- TBmfreqSulawesi[, c("MAF")]
TBmfreqSulawesi <- TBmfreqSulawesi %>% 
  rename("Sulawesi"="MAF")
TBmfreqSumAna <- TBmfreqSumAna[, c("MAF")]
TBmfreqSumAna <- TBmfreqSumAna %>% 
  rename("Sumba (Ana)"="MAF")
TBmfreqSumatra <- TBmfreqSumatra[, c("MAF")]
TBmfreqSumatra <- TBmfreqSumatra %>% 
  rename("Sumatra"="MAF")
TBmfreqSumRin <- TBmfreqSumRin[, c("MAF")]
TBmfreqSumRin <- TBmfreqSumRin %>% 
  rename("Sumba (Rin)"="MAF")
TBmfreqSumWun <- TBmfreqSumWun[, c("MAF")]
TBmfreqSumWun <- TBmfreqSumWun %>% 
  rename("Sumba (Wun)"="MAF")
TBmfreqTaiwan <- TBmfreqTaiwan[, c("MAF")]
TBmfreqTaiwan <- TBmfreqTaiwan %>% 
  rename("Taiwan"="MAF")
TBmfreqTimKam <- TBmfreqTimKam[, c("MAF")]
TBmfreqTimKam <- TBmfreqTimKam %>% 
  rename("Timor (Kam)"="MAF")
TBmfreqTimULa <- TBmfreqTimULa[, c("MAF")]
TBmfreqTimULa <- TBmfreqTimULa %>% 
  rename("Timor (ULa)"="MAF")
TBmfreqVietnam <- TBmfreqVietnam[, c("MAF")]
TBmfreqVietnam <- TBmfreqVietnam %>% 
  rename("Vietnam"="MAF")
TBmfreqYRI <- TBmfreqYRI[, c("MAF")]
TBmfreqYRI <- TBmfreqYRI %>% 
  rename("Yuruba (Nigeria)"="MAF")

TBmall <- cbind(TBmref, TBmfreqAlor, TBmfreqBali, TBmfreqBEB, TBmfreqBorneo, TBmfreqCDX, TBmfreqCEU, TBmfreqCHB, TBmfreqCHS, TBmfreqFloBamAF, TBmfreqFloBenAF, TBmfreqFloRamAF, TBmfreqGIH, TBmfreqITU, TBmfreqJavaAF, TBmfreqJPT, TBmfreqKHV, TBmfreqLembat, TBmfreqMalay, TBmfreqMentaw, TBmfreqNias, TBmfreqPantar, TBmfreqPHNegr, TBmfreqPHOther, TBmfreqPHIgorTR, TBmfreqPJL, TBmfreqPNGBai, TBmfreqPNGCAF, TBmfreqPNGHAF, TBmfreqPNGNas, TBmfreqSTU, TBmfreqSulawesi, TBmfreqSumAna, TBmfreqSumatra, TBmfreqSumRin, TBmfreqSumWun, TBmfreqTaiwan, TBmfreqTimKam, TBmfreqTimULa, TBmfreqVietnam, TBmfreqYRI)
write.csv(TBmall, "TBmall.csv", row.names = FALSE)

##Creating pharmacogenetic tables
#Load data
MalCli <- read.csv("Malariaall.csv", header=TRUE)

#Rename data
MalCli <- MalCli %>%
  rename(
    "Dai (China)" = Dai..China.,
    "Han (China)" = Han..China.,
    "Southern Han (China)" = Southern.Han..China.,
    "Flores Bama" = Flores..Bama.,
    "Flores Bena" = Flores..Bena.,
    "Flores Rampasasa" = Flores..Rampasasa.,
    "Gujarati (India)" = Gujarati..India.,
    "Telugu (India)" = Telugu..India.,
    "KHV (Vietnam)" = KHV..Vietnam.,
    "Negritos (Philippines)" = Negritos..Philippines.,
    "Kankanaey (Philippines)" = Kankanaey..Philippines,
    "Non-Kankanaey (Philippines)" = Non.Kankanaey..Philippines.,
    "Punjabi (Pakistan)" = Punjabi..Pakistan.,
    "Papua New Guinea (Baining)" = PNG.Baining,
    "Papua New Guinea (Coastal)" = PNG.Coastal,
    "Papua New Guinea (Highland)" = PNG.Highlands,
    "Papua New Guinea (Nasioi)" = PNG.Nasioi,
    "Tamil (Sri Lanka)" = Tamil..Sri.Lanka.,
    "Sumba (Ana)" =Sumba..Ana.,
    "Sumba (Rin)" = Sumba..Rin.,
    "Sumba (Wun)" = Sumba..Wun.,
    "Timor (Kam)" = Timor..Kam.,
    "Timor (ULa)" = Timor..ULa.,
    "Yuruba (Nigeria)" = Yuruba..Nigeria.
  )
  
#Remove non-Indonesian populations
MalCli <- MalCli[, c("SNP","Alor", "Bali", "Borneo", "Flores Bama", "Flores Bena", "Flores Rampasasa", "Java", "Lembata", "Malay", "Mentawai", "Nias", "Pantar", "Papua New Guinea (Baining)", "Papua New Guinea (Coastal)", "Papua New Guinea (Highland)", "Papua New Guinea (Nasioi)", "Sulawesi", "Sumatra", "Sumba (Ana)", "Sumba (Rin)", "Sumba (Wun)", "Timor (Kam)", "Timor (ULa)")]

#Renaming column headers 
MalCli <- data.frame(t(MalCli))
MalCli <- tibble::rownames_to_column(MalCli,"Population")
MalCli <- janitor::row_to_names(MalCli, row_number = 1)
MalCli <- MalCli %>%
  rename(
    "Population"=SNP,
  )
  
#Making data numeric
MalCli <- MalCli %>%
  mutate_at(c(2:20), as.numeric) 

#Manually removing alleles below exclusion threshold

MalCli$`HLA_B*53`=NULL
MalCli$`HLA_B*53:01`=NULL
MalCli$`HLA_A*33:01`=NULL
MalCli$`HLA_A*66:02`=NULL
MalCli$`HLA_B*78:01`=NULL
MalCli$`HLA_DRB1*08:09`=NULL
MalCli$`HLA_DQB1*02:03`=NULL
MalCli$`HLA_DRB1*04:01`=NULL
MalCli$`HLA_A*30:01`=NULL
MalCli$`HLA_A*74`=NULL
MalCli$`HLA_DRB1*01:01`=NULL
MalCli$`HLA_DPB1*17:01`=NULL
MalCli$`HLA_DRB1*10:01`=NULL
MalCli$`HLA_DRB1*13:02`=NULL
MalCli$`HLA_B*35:01`=NULL

#Removing "HLA" prefix
colnames(MalCli) <- sub("^HLA_", "", colnames(MalCli))

#Creating table
MalCli %>%
  gt() %>%
  data_color(columns = 5:6, 
             colors = col_numeric(palette = c("white","darkgreen"),
                                  domain = c(0,1))) %>%
  data_color(columns = 2:4, 
             colors = col_numeric(palette = c("white","darkred"),
                                  domain = c(0,1))) %>%
  cols_align(columns=2:6,
             align = "center") %>%
  fmt_number(columns = 2:6,
             decimals = 3) %>%
  gtsave("Malaria-Table.pdf")

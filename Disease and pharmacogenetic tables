  gt() %>%
  data_color(columns = 2|4|6:7, 
             colors = col_numeric(palette = c("white","darkgreen"),
                                  domain = c(0,1))) %>%
  data_color(columns = 3|5, 
             colors = col_numeric(palette = c("white","darkred"),
                                  domain = c(0,1))) %>%
  cols_align(columns=2:7,
             align = "center") %>%
  fmt_number(columns = 2:7,
             decimals = 3) %>%
  gtsave("Malaria-Vaccine-Table.pdf")


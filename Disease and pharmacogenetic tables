    "Flores Rampasasa" = Flores..Rampasasa.,
    "Gujarati (India)" = Gujarati..India.,
    "Telugu (India)" = Telugu..India.,
    "KHV (Vietnam)" = KHV..Vietnam.,
    "Negritos (Philippines)" = Negritos..Philippines.,
    "Kankanaey (Philippines)" = Kankanaey..Philippines,
    "Non-Kankanaey (Philippines)" = Non.Kankanaey..Philippines.,
    "Punjabi (Pakistan)" = Punjabi..Pakistan.,
    "Papua New Guinea (Baining)" = PNG.Baining,
    "Papua New Guinea (Coastal)" = PNG.Coastal,
    "Papua New Guinea (Highland)" = PNG.Highlands,
    "Papua New Guinea (Nasioi)" = PNG.Nasioi,
    "Tamil (Sri Lanka)" = Tamil..Sri.Lanka.,
    "Sumba (Ana)" =Sumba..Ana.,
    "Sumba (Rin)" = Sumba..Rin.,
    "Sumba (Wun)" = Sumba..Wun.,
    "Timor (Kam)" = Timor..Kam.,
    "Timor (ULa)" = Timor..ULa.,
    "Yuruba (Nigeria)" = Yuruba..Nigeria.
  )
    
#Remove non-Indonesian populations
TBCli <- TBCli[, c("SNP","Alor", "Bali", "Borneo", "Flores Bama", "Flores Bena", "Flores Rampasasa", "Java", "Lembata", "Malay", "Mentawai", "Nias", "Pantar", "Papua New Guinea (Baining)", "Papua New Guinea (Coastal)", "Papua New Guinea (Highland)", "Papua New Guinea (Nasioi)", "Sulawesi", "Sumatra", "Sumba (Ana)", "Sumba (Rin)", "Sumba (Wun)", "Timor (Kam)", "Timor (ULa)")]

#Renaming column headers 
TBCli <- data.frame(t(TBCli))
TBCli <- tibble::rownames_to_column(TBCli,"Population")
TBCli <- janitor::row_to_names(TBCli, row_number = 1)
TBCli <- TBCli %>%
  rename(
    "Population"=SNP,
  )
  
#Making data numeric
TBCli <- TBCli %>%
  mutate_at(c(2:23), as.numeric) 

#Manually removing alleles below exclusion threshold
TBCli$`HLA_A*02:02`=NULL
TBCli$`HLA_A*30:02`=NULL
TBCli$`HLA_B*58:02`=NULL
TBCli$`HLA_DQB1*06:09`=NULL
TBCli$`HLA_DRB1*10`=NULL

#Removing "HLA" prefix
colnames(TBCli) <- sub("^HLA_", "", colnames(TBCli))

#Creating table
TBCli %>%
  gt() %>%
  data_color(columns = 2:4 | 8:9 |13:15|17 , 
             colors = col_numeric(palette = c("white","darkgreen"),
                                  domain = c(0,1))) %>%
  data_color(columns = 5:7| 10:12 |16 |18,
             colors = col_numeric(palette = c("white","red"),
                                  domain = c(0,1))) %>%
  cols_align(columns=2:18,
             align = "center") %>%
  fmt_number(columns = 2:18,
             decimals = 3)%>%
  gtsave("TB-Table.pdf", zoom=1)


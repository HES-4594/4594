HBVvall <- cbind(HBVvref, HBVvfreqAlor, HBVvfreqBali, HBVvfreqBEB, HBVvfreqBorneo, HBVvfreqCDX, HBVvfreqCEU, HBVvfreqCHB, HBVvfreqCHS, HBVvfreqFloBamAF, HBVvfreqFloBenAF, HBVvfreqFloRamAF, HBVvfreqGIH, HBVvfreqITU, HBVvfreqJavaAF, HBVvfreqJPT, HBVvfreqKHV, HBVvfreqLembat, HBVvfreqMalay, HBVvfreqMentaw, HBVvfreqNias, HBVvfreqPantar, HBVvfreqPHNegr, HBVvfreqPHOther, HBVvfreqPHIgorTR, HBVvfreqPJL, HBVvfreqPNGBai, HBVvfreqPNGCAF, HBVvfreqPNGHAF, HBVvfreqPNGNas, HBVvfreqSTU, HBVvfreqSulawesi, HBVvfreqSumAna, HBVvfreqSumatra, HBVvfreqSumRin, HBVvfreqSumWun, HBVvfreqTaiwan, HBVvfreqTimKam, HBVvfreqTimULa, HBVvfreqVietnam, HBVvfreqYRI)
write.csv(HBVvall, "HBVvall.csv", row.names = FALSE)


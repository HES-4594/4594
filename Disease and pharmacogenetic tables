#########################################
###Disease and pharmacogenetic tables ###
#########################################

##Create HLA frequency table
setwd()
#Calculate MAF + stratify by population  
#HLA
system(str_c("plink --bfile HUDHLAonly --autosome ",
             "--keep PopulationLabels.txt --nonfounders ",
             "--freq --within PopulationLabels.txt ",
             "--out HLASnpFrequencies"))
             
##HLA frequency table by population
#Alor 
Alor <- read.table("PopulationLabels.txt", header=TRUE)
Alor <- filter(Alor, FID=="AlorAF")
write.table(Alor, file = "AlorPopLab.txt", quote=FALSE, row.names = FALSE)
system(str_c("plink --bfile HUDHLAonly --autosome ",
             "--keep AlorPopLab.txt --nonfounders ",
             "--freq --within AlorPopLab.txt ",
             "--out HLAAlorSnpFrequencies"))

#Bali 
Bali <- read.table("PopulationLabels.txt", header=TRUE)
Bali <- filter(Bali, FID=="BaliAF")

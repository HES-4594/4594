#Creating table
HBVM %>%
  gt() %>%
  data_color(columns = 2|6:7|9, 
             colors = col_numeric(palette = c("white","darkgreen"),
                                  domain = c(0,1))) %>%
  data_color(columns = 3:5|8|10:11,  
             colors = col_numeric(palette = c("white","darkred"),
                                  domain = c(0,1))) %>%
  cols_align(columns=2:11,
             align = "center") %>%
  fmt_number(columns = 2:11,
             decimals = 3) %>%
  gtsave("HBV-Medicine-Table.pdf", zoom=1)

##HBV - vaccine

#Load data
HBVV <- read.csv("HBVVall.csv", header=TRUE)

#Rename data
HBVV <- HBVV %>%
  rename(
    "Dai (China)" = Dai..China.,
    "Han (China)" = Han..China.,
    "Southern Han (China)" = Southern.Han..China.,
    "Flores Bama" = Flores..Bama.,
    "Flores Bena" = Flores..Bena.,
    "Flores Rampasasa" = Flores..Rampasasa.,
    "Gujarati (India)" = Gujarati..India.,
    "Telugu (India)" = Telugu..India.,
    "KHV (Vietnam)" = KHV..Vietnam.,
    "Negritos (Philippines)" = Negritos..Philippines.,
    "Kankanaey (Philippines)" = Kankanaey..Philippines,
    "Non-Kankanaey (Philippines)" = Non.Kankanaey..Philippines.,
    "Punjabi (Pakistan)" = Punjabi..Pakistan.,
    "Papua New Guinea (Baining)" = PNG.Baining,
    "Papua New Guinea (Coastal)" = PNG.Coastal,
    "Papua New Guinea (Highland)" = PNG.Highlands,
    "Papua New Guinea (Nasioi)" = PNG.Nasioi,
    "Tamil (Sri Lanka)" = Tamil..Sri.Lanka.,
    "Sumba (Ana)" =Sumba..Ana.,
    "Sumba (Rin)" = Sumba..Rin.,
    "Sumba (Wun)" = Sumba..Wun.,
    "Timor (Kam)" = Timor..Kam.,
    "Timor (ULa)" = Timor..ULa.,
    "Yuruba (Nigeria)" = Yuruba..Nigeria.
  )

#Remove non-Indonesian populations
HBVV <- HBVV[, c("SNP","Alor", "Bali", "Borneo", "Flores Bama", "Flores Bena", "Flores Rampasasa", "Java", "Lembata", "Malay", "Mentawai", "Nias", "Pantar", "Papua New Guinea (Baining)", "Papua New Guinea (Coastal)", "Papua New Guinea (Highland)", "Papua New Guinea (Nasioi)", "Sulawesi", "Sumatra", "Sumba (Ana)", "Sumba (Rin)", "Sumba (Wun)", "Timor (Kam)", "Timor (ULa)")]

#Renaming column headers 
HBVV <- data.frame(t(HBVV))
HBVV <- tibble::rownames_to_column(HBVV,"Population")
HBVV <- janitor::row_to_names(HBVV, row_number = 1)
HBVV <- HBVV %>%
  rename(
    "Population"=SNP,
  )

#Making data numeric
HBVV <- HBVV %>%
  mutate_at(c(2:42), as.numeric) 

#Manually removing alleles below exclusion threshold
HBVV$`HLA_DQB1*06:04`=NULL
HBVV$`HLA_DQB1*02:02`=NULL
HBVV$`HLA_DPB1*11:01`=NULL
HBVV$`HLA_DRB1*04:01`=NULL
HBVV$`HLA_DRB1*08:02`=NULL
HBVV$`HLA_DRB1*13:02`=NULL
HBVV$`HLA_DRB1*14:03`=NULL
HBVV$`HLA_DRB1*01`=NULL
HBVV$`HLA_DRB1*04:06`=NULL
HBVV$`HLA_DRB1*10`=NULL

#Removing "HLA" prefix
colnames(HBVV) <- sub("^HLA_", "", colnames(HBVV))

#Creating table
HBVVDet %>%
  gt() %>%
  data_color(columns = 2:17, 
             colors = col_numeric(palette = c("white","darkred"),
                                  domain = c(0,1)))%>% 
  cols_align(columns=2:17,
             align = "center") %>%
  fmt_number(columns = 2:17,
             decimals = 3) %>%
  gtsave("HBVDet-Vaccine-Table.pdf")

HBVVProt %>%
  gt() %>%
  data_color(columns = 2:16, 
             colors = col_numeric(palette = c("white","darkgreen"),
                                  domain = c(0,1))) %>%
  cols_align(columns=2:16,
             align = "center") %>%
  fmt_number(columns = 2:16,
             decimals = 3) %>%
  gtsave("HBVPro-Vaccine-Table.pdf")

#Creating table
TBM %>%
  gt() %>%
  data_color(columns = 3, 
             colors = col_numeric(palette = c("white","darkgreen"),
                                  domain = c(0,1))) %>%
  data_color(columns = 2|4, 
             colors = col_numeric(palette = c("white","darkred"),
                                  domain = c(0,1))) %>%
  cols_align(columns=2:4,
             align = "center") %>%
  fmt_number(columns = 2:4,
             decimals = 3) %>%
  gtsave("TB-Medicine-Table.pdf")


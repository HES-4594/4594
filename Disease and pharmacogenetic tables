#Load data
TBCli <- read.csv("TBall.csv", header=TRUE)

#Rename data
TBCli <- TBCli %>%
  rename(
    "Dai (China)" = Dai..China.,
    "Han (China)" = Han..China.,
    "Southern Han (China)" = Southern.Han..China.,
    "Flores Bama" = Flores..Bama.,
